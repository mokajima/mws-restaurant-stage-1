  "version": "1.0.0",
  "description": "Restaurant Reviews App is a website which is responsive, accessible, and available offline.",
  "main": "sw.js",
  "scripts": {
    "node-sass": "node-sass --watch --output-style compressed scss/styles.scss css/styles.css",
    "postcss": "postcss css/styles.css --no-map --output css/styles.min.css --watch",
    "start": "npm run node-sass & npm run postcss"
